    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.6",
    "next": "12.2.5",
    "nextjs-breadcrumbs": "^1.1.9",
    "react": "18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "18.2.0",
    "react-glider": "^4.0.2",
    "react-paginate": "^8.1.3",
